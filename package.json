{
  "name": "home-server",
  "version": "1.6.0",
  "description": "Smart Home server",
  "main": "src/index.js",
  "engines": {
    "node": "12.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/demian85/home-server.git"
  },
  "scripts": {
    "postinstall": "npm run build",
    "build": "webpack -p --progress",
    "start": "node src/index.js",
    "dev-client": "webpack --progress --watch",
    "dev-server": "nodemon src/index.js",
    "dev": "concurrently -k -c 'green,magenta' 'npm:dev-client' 'npm:dev-server'",
    "lint": "eslint ./src",
    "init:config": "node src/server/init.js",
    "format": "prettier --write './src/**/*.{js,jsx,ts,tsx}'"
  },
  "keywords": [
    "sonoff",
    "smart",
    "home",
    "server",
    "esp8266",
    "wemos",
    "domotics",
    "automation",
    "shelly"
  ],
  "author": "Demian Rodriguez",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "feels": "^3.0.0",
    "hbs": "^4.1.1",
    "lodash": "^4.17.15",
    "luxon": "^1.23.0",
    "mqtt": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "redis": "^2.8.0",
    "request": "^2.88.2",
    "solar-calc": "^1.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-router-dom": "^4.3.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
